// Vesuvius Programming Language : Example / Factorial



extern std;

let stdout = std::stdout;
let stdin  = std::stdin;
let type   = std::types;

#[entry]
#[public]
func main() : type::Void {
    stdout::write("Enter a number: ");
    stdout::flush();
    let input = stdin::input();
    stdout::writeln("\nYou inputted: " + input + ".");
};
